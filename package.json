{
  "name": "data-quality-dashboard",
  "version": "0.1.0",
  "description": "A dashboard for tracking and displaying open data publication quality.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "mocha --require babel-core/register tests/",
    "build:front": "webpack --config webpack.config.development.js",
    "build:front:min": "webpack --config webpack.config.production.js",
    "build": "npm run build:front && npm run build:front:min",
    "prepublish": "npm run build",
    "postinstall": "npm run build",
    "review": "jscs lib test",
    "fix": "jscs lib test --fix"
  },
  "keywords": [
    "open data",
    "open spending",
    "open knowledge",
    "data packages",
    "frictionless data"
  ],
  "dependencies": {
    "babel-core": "^6.1.2",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "bluebird": "2.9.24",
    "bootstrap": "^3.3.5",
    "bootstrap-sass": "3.3.4",
    "chart.js": "^1.0.2",
    "csv": "0.4.6",
    "express": "4.13.3",
    "express-react-views": "0.9.0",
    "history": "^1.12.5",
    "isomorphic-fetch": "^2.1.1",
    "jquery": "^2.1.4",
    "lodash": "4.14.1",
    "marked": "0.3.5",
    "moment": "^2.14.1",
    "nconf": "0.8.2",
    "node-cache": "3.2.1",
    "object-assign": "2.0.0",
    "react": "0.14.0",
    "react-bootstrap": "0.29.4",
    "react-bootstrap-async-autocomplete": "0.0.3",
    "react-chartjs": "0.6.0",
    "react-dom": "0.14.0",
    "react-redux": "3.1.0",
    "react-router": "^1.0.0-rc3",
    "react-tools": "0.13.3",
    "redux": "3.0.2",
    "redux-actions": "^0.8.0",
    "redux-logger": "2.0.4",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "throng": "1.0.1"
